{
  "name": "file-uploader",
  "version": "1.0.0",
  "description": "Facilitates file uploads.",
  "scripts": {
    "clean": "rimraf dist",
    "copy-files": "./node_modules/copyfiles/copyfiles --verbose --error --up 1 src-client/**/*.html src-client/**/*.ico src-client/**/*.jpg src-client/**/*.png dist/client",
    "precompile": "eslint -c .eslintrc.js --fix --ext .ts src",
    "compile": "./node_modules/webpack-cli/bin/cli.js --config webpack.config.js && ./node_modules/webpack-cli/bin/cli.js --config webpack-client.config.js",
    "build": "npm run compile",
    "postbuild": "npm run copy-files",
    "dev": "cross-env NODE_ENV=development ./node_modules/tsc-watch/index.js --project . --outDir ./dist --onSuccess \"nodemon\"",
    "start": "npm run build && node dist/index.js",
    "package": "node ./package-gzip.js",
    "install": "cross-env NODE_ENV=production npm run build && npm run package",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": [
      "./src",
      "./src-client",
      "./node_modules"
    ],
    "ext": "ts,js,html,css,ico",
    "delay": 2500
  },
  "keywords": [
    "file"
  ],
  "author": "Paul Gundarapu",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/formidable": "^1.2.5",
    "@types/jquery": "^3.5.14",
    "@types/serve-favicon": "^2.5.3",
    "@types/toastify-js": "^1.11.1",
    "@types/uuid": "^8.3.4",
    "bulma": "^0.8.2",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "formidable": "^1.2.6",
    "jquery": "^3.6.1",
    "jquery-blockui": "^2.7.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.29.4",
    "mv": "^2.1.1",
    "pretty-bytes": "^5.3.0",
    "rimraf": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "targz": "^1.0.1",
    "toastify-js": "^1.12.0",
    "uuid": "^8.3.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/jquery.blockui": "^0.0.29",
    "@types/lodash": "^4.14.184",
    "@types/mv": "^2.1.2",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "concurrently": "^7.3.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.19",
    "postcss-loader": "^7.0.1",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
